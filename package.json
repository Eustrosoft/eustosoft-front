{
  "name": "eustrosoft-front",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start:login": "npx nx run login:serve",
    "start:dispatcher": "npx nx run dispatcher:serve",
    "start:explorer": "npx nx run explorer:serve",
    "build:all:stage": "npx nx run-many --target=build --configuration=stage --projects=login,dispatcher,explorer --parallel=3",
    "build:all:production": "npx nx run-many --target=build --configuration=production --projects=login,dispatcher,explorer --parallel=3",
    "build:affected:stage": "npx nx affected --target=build --configuration=stage --base=origin/develop --head=HEAD --parallel --max-parallel=3",
    "build:affected:production": "npx nx affected --target=build --configuration=production --base=origin/master --head=HEAD --parallel --max-parallel=3",
    "nx:graph": "npx nx graph"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "15.2.2",
    "@angular/cdk": "15.2.2",
    "@angular/common": "15.2.2",
