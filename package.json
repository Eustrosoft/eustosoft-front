{
  "name": "eustrosoft-front",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start:login": "npx nx run login:serve",
    "start:dispatcher": "npx nx run dispatcher:serve",
    "start:explorer": "npx nx run explorer:serve",
    "build:login:stage": "npx nx run login:build --configuration=stage",
    "build:dispatcher:stage": "npx nx run dispatcher:build --configuration=stage",
    "build:explorer:stage": "npx nx run explorer:build --configuration=stage",
    "build:login:production": "npx nx run login:build --configuration=production",
    "build:dispatcher:production": "npx nx run dispatcher:build --configuration=production",
    "build:explorer:production": "npx nx run explorer:build --configuration=production",
    "build:all:stage": "npx nx run-many --target=build --configuration=stage --projects=login,dispatcher,explorer --parallel=3",
    "build:all:production": "npx nx run-many --target=build --configuration=production --projects=login,dispatcher,explorer --parallel=3",
    "nx:graph": "npx nx graph"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~15.1.0",
    "@angular/cdk": "~15.1.0",
    "@angular/common": "~15.1.0",
    "@angular/compiler": "~15.1.0",
    "@angular/core": "~15.1.0",
    "@angular/forms": "~15.1.0",
    "@angular/material": "~15.1.0",
    "@angular/platform-browser": "~15.1.0",
    "@angular/platform-browser-dynamic": "~15.1.0",
    "@angular/router": "~15.1.0",
    "@nrwl/angular": "15.7.1",
    "bootstrap": "^5.2.3",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~15.1.0",
    "@angular-eslint/eslint-plugin": "~15.0.0",
    "@angular-eslint/eslint-plugin-template": "~15.0.0",
    "@angular-eslint/template-parser": "~15.0.0",
    "@angular/cli": "~15.1.0",
    "@angular/compiler-cli": "~15.1.0",
    "@angular/language-service": "~15.1.0",
    "@nrwl/cypress": "15.7.1",
    "@nrwl/eslint-plugin-nx": "15.7.1",
    "@nrwl/jest": "15.7.1",
    "@nrwl/linter": "15.7.1",
    "@nrwl/nx-cloud": "15.0.3",
    "@nrwl/workspace": "15.7.1",
    "@types/jest": "28.1.1",
    "@types/node": "16.11.7",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "cypress": "^12.2.0",
    "eslint": "~8.15.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "jest": "28.1.1",
    "jest-environment-jsdom": "28.1.1",
    "jest-preset-angular": "~12.2.3",
    "nx": "15.7.1",
