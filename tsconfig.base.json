{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "ES2022",
    "module": "ES2022",
    "lib": ["ES2022", "DOM"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@eustrosoft-front/app-config": ["libs/app-config/index.ts"],
      "@eustrosoft-front/common-ui": ["libs/common-ui/src/index.ts"],
      "@eustrosoft-front/config": ["libs/config/src/index.ts"],
      "@eustrosoft-front/core": ["libs/core/src/index.ts"],
      "@eustrosoft-front/dao-ts": ["libs/dao-ts/src/index.ts"],
      "@eustrosoft-front/dic": ["libs/dic/src/index.ts"],
      "@eustrosoft-front/dispatcher-lib": ["libs/dispatcher-lib/src/index.ts"],
      "@eustrosoft-front/explorer-lib": ["libs/explorer-lib/src/index.ts"],
      "@eustrosoft-front/login-lib": ["libs/login-lib/src/index.ts"],
      "@eustrosoft-front/msg-lib": ["libs/msg-lib/src/index.ts"],
      "@eustrosoft-front/security": ["libs/security/src/index.ts"]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
