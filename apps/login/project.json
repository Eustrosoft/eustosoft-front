{
  "name": "login",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/login/src",
  "prefix": "eustrosoft-front",
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/login",
        "index": "apps/login/src/index.html",
        "main": "apps/login/src/main.ts",
        "baseHref": "/login/",
        "polyfills": ["zone.js", "apps/login/src/polyfills.ts"],
        "tsConfig": "apps/login/tsconfig.app.json",
        "stylePreprocessorOptions": {
          "includePaths": ["libs/common-ui/src/lib/styles"]
        },
        "inlineStyleLanguage": "scss",
        "assets": ["apps/login/src/favicon.ico", "apps/login/src/assets"],
        "styles": [
          "@angular/material/prebuilt-themes/deeppurple-amber.css",
          "node_modules/bootstrap/scss/bootstrap.scss",
          "libs/common-ui/src/lib/material/css/roboto.css",
          "libs/common-ui/src/lib/material/css/material-symbols-outlined.css",
          "apps/login/src/styles.scss"
        ],
        "scripts": []
      },
      "configurations": {
        "production": {
          "assets": [
            "apps/login/src/favicon.ico",
            {
              "glob": "config.json",
              "input": "libs/config/src/lib/configurations/prod",
              "output": "/"
            },
            {
              "glob": "*.json",
              "input": "apps/login/src/assets/i18n",
              "output": "/assets/i18n/"
            }
          ],
          "outputHashing": "all",
          "optimization": true,
          "sourceMap": false,
          "namedChunks": false,
          "aot": true,
          "extractLicenses": true,
          "vendorChunk": false,
          "buildOptimizer": true
        },
        "stage": {
          "assets": [
            "apps/login/src/favicon.ico",
            {
              "glob": "config.json",
              "input": "libs/config/src/lib/configurations/stage",
              "output": "/"
            },
            {
              "glob": "*.json",
              "input": "apps/login/src/assets/i18n",
              "output": "/assets/i18n/"
            }
          ]
        },
        "development": {
          "assets": [
            "apps/login/src/favicon.ico",
            {
              "glob": "config.json",
              "input": "libs/config/src/lib/configurations/dev",
              "output": "/"
            },

            {
              "glob": "*.json",
              "input": "apps/login/src/assets/i18n",
              "output": "/assets/i18n/"
            }
          ],
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true,
          "localize": ["en-US"]
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "options": {
        "port": 4200
      },
      "configurations": {
        "production": {
          "browserTarget": "login:build:production"
        },
        "development": {
          "browserTarget": "login:build:development",
          "proxyConfig": "apps/login/proxy.conf.json"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "browserTarget": "login:build",
        "outputPath": "apps/login/src/locale",
        "outFile": "en-US.json",
        "format": "json"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/login/**/*.ts", "apps/login/**/*.html"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/login/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    }
  },
  "tags": []
}
